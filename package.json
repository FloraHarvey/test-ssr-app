  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "npx webpack -c ./webpack.client.config.ts",
    "build:client:prod": "npm run build:client -- --env PRODUCTION",
    "start:client": "npx webpack server -c ./webpack.client.config.ts --env HOT",
    "build:server": "npx webpack -c ./webpack.server.config.ts",
    "build:server:prod": "npm run build:server -- --env PRODUCTION",
    "start:server": "npx concurrently \"npm run build:client -- -w\" \"npm run build:server -- -w\" \"npx nodemon ./dist/app.js\"",
    "start:server:prod": "npx concurrently \"npm run build:client:prod -- -w\" \"npm run build:server:prod -- -w\" \"npx nodemon ./dist/app.js\"",
    "build": "npm run build:server:prod && npm run build:client:prod"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.3.1",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.14",
    "react-refresh": "^0.10.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  }
}
